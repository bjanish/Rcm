<?php

/** @var \Rcm\Entity\PluginInstance $newInstance */
$jsonReturn['display'] = $this->render($this->newInstance->getView());
$jsonReturn['display'] .= $this->headScript();
$jsonReturn['display'] .= $this->headLink();
